    },
    "dockerFile": "/Dockerfile",
    "source": "frontend",
    "process": "web",
    "size": "S1",
    "config": {},
    "ports": [
        {
            "internal": 80,
            "type": "http"
        }
    ],
    "links": [],
    "envs": [
        {
            "type": "as",
            "key": "PUID",
            "value": "0"
        },
        {
